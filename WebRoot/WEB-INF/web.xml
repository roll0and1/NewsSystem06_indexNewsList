<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.qiangge.web.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.qiangge.web.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ToAdmin</servlet-name>
    <servlet-class>com.qiangge.web.ToAdmin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ToEditor</servlet-name>
    <servlet-class>com.qiangge.web.ToEditor</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ToCreateNews</servlet-name>
    <servlet-class>com.qiangge.web.ToCreateNews</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateNews</servlet-name>
    <servlet-class>com.qiangge.web.CreateNews</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>com.qiangge.web.IndexServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ToAdmin</servlet-name>
    <url-pattern>/toAdmin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ToEditor</servlet-name>
    <url-pattern>/toEditor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ToCreateNews</servlet-name>
    <url-pattern>/toCreateNews</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateNews</servlet-name>
    <url-pattern>/createNews</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
</web-app>